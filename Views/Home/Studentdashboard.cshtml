
@model List<Dashboard>

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

<head>
<link rel="stylesheet" href="~/css/Studentdashboard.css" asp-append-version="true" />
</head>

		
		 
	  <div class="container">
		<nav>
		  <div><br />
			<div><a href="#" class="logo">
			  <img src="~/download.jpg" alt="">
			  <span class="nav-item">DashBoard</span>
			</a></div><br /><br />
			<div><a href="Index">
			  <i class="fas fa-home"></i>
			  <span class="nav-item">Home</span>
			</a></div>
			<br />
			<div>
				<a href="Placement" class="placement">
					<i class="fas fa-solid fa-user"></i>
					<span class="nav-item">Placement</span>
				</a>
			</div>
			<br />
			<div>
				<a href="Review" class="review">
					<i class="fas fa-solid fa-star"></i>
					<span class="nav-item">Review Us</span>
				</a>
			</div>
			<br />
			<div>
				<a href="Logout" class="logout">
			  <i class="fas fa-sign-out-alt"></i>
			  <span class="nav-item">Log out</span>
			</a></div>
		  </div>
		</nav>


	
	   
	<!-- main -->
	<section class="main">
		<div class="main-top">
			<!-- search bar -->
			<h1><b>  Courses </b></h1>

		</div>

		<div class="main-skills">
			@foreach (var course in Model)
			{
				<div class="card">
					<h3>@course.Name</h3>
					 
					<button onclick="location.href='@Url.Action("GetCourseDetails", "Home", new { courseId = course.Id })'">Get Started</button>
				</div>
			}
		
		<br>
		</div>
		<br />
		
		
			
	</section>
</div>